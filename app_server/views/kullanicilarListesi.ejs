
<h1>Kullanicilar</h1>
<div class="container">
<table class="table table-hover">
    <thead>
        <th>#</th>
        <th>Ad</th>
        <th>Soyad</th>
        <th>Kullanıcı Adı</th>
        <th>İşlemler</th>
    </thead>
    <tbody>
        <%for(var i=0; i<kullanicilar.length;i++){%>
            <tr>
                <th><%=i+1%></th>
                <th><%=kullanicilar[i].ad%></th>
                <th><%=kullanicilar[i].soyad%></th>
                <th><%=kullanicilar[i].kullaniciAdi%></th>
                <th><a href="/login/kullanicisil/<%=kullanicilar[i].ad%>">Sil</a></th>
            </tr>
        <%}%>
    </tbody>
</table>
</div>